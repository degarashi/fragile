{
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"globals": {},
	"extends": "eslint:recommended",
	"parserOptions": {
		"sourceType": "module",
		"ecmaFeatures": {}
	},
	"rules": {
		"no-unused-vars": [
			1,
			{
				"vars": "all",
				"args": "after-used"
			}
		],
		"indent": [
			"error",
			"tab"
		],
		"linebreak-style": [
			"error",
			"unix"
		],
		"quotes": [
			"error",
			"double"
		],
		"semi": [
			"error",
			"always"
		]
	}
}
